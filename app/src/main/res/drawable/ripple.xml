<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#141413">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#e09410" />
        </shape>
    </item>

    <item
        android:bottom="0dp"
    android:left="0dp"
    android:right="0dp"
    android:top="0dp">
    <shape>
        <stroke
            android:width="3dp"
        android:color="#000000" />
    </shape>
</item>
</ripple>